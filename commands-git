echo "# comandosgit" >> README.md
git init
git add README.md
git commit -m "first commit"
git remote add origin https://github.com/silverioalejandro/comandosgit.git
git push -u origin master

elistar comit
git log

establecer el usuario
    local
git config user.name "silverioalejandro"

    global
git config --globlal user.name "silverioalejandro"

establecer el gmail
    local
git config user.email "silverioserranoj@gmail.com"

    global
git config --globlal user.email "silverioserranoj@gmail.com"

establecer el password

    local
git config user.password "xxx"

    global
git config --globlal user.password "xxx"
    
para visualizar los commit
    git log --oneline

para visualizar el estado
    git status

para agregar las modificaciones al stage
    git add .

para crear commit 
    git commit -m "lo que se progamo"

para subirlo a github
    git push origin master
    
para bajar de github
    git pull origin master
    
para clonar proyecto
    git clone ruta-url

para crear una rama
    git checkout -b feature/nombre-de-lo-que-se-cambia

en main actualiza los datos de la rama
    git merge nombreDeLaRama

para borrar todos los cambios 
    git checkout .

para dirigirme a la carpeta en el terminator
    cd Documentos/web-silverio
    

para subirlo a la web
    git push heroku master

para realizar deploy heroku
    heroku login

por el terminal te diriges a el operador que quieras abrir
    ejemplo: code /var/www/comandos/

